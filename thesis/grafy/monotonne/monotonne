#!/usr/bin/gnuplot
load 'include'

set log xy
set xrange [1:2000]

set output 'sgnTest0.01.eps'
set xlabel "pocet krokov"
set ylabel "95% spol."
set title 'Sgn {/Symbol l}=0.01'
plot 'SgnTest0.01.dat' usi 1:2 w points pt 3 ps 1.5 title 'Robbins-Monro',\
     'SgnTest0.01.dat' usi 1:3 w points pt 4 ps 1.5 title 'Newton',\
     'SgnTest0.01.dat' usi 1:4 w points pt 6 ps 1.5 title 'regula falsi'

set output 'sgnTest0.1.eps'
set xlabel "pocet krokov"
set ylabel "95% spol."
set title 'Sgn {/Symbol l}=0.1'
