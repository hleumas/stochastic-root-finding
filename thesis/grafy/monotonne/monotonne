     'SgnTest0.4.dat' usi 1:3 w points pt 4 ps 1.5 title 'Newton',\
     'SgnTest0.4.dat' usi 1:4 w points pt 6 ps 1.5 title 'regula falsi'

set output 'sgnTest0.6.eps'
set xlabel "pocet krokov"
