     'SgnTest0.2.dat' usi 1:3 w points pt 4 ps 1.5 title 'Newton',\
     'SgnTest0.2.dat' usi 1:4 w points pt 6 ps 1.5 title 'regula falsi'

set output 'sgnTest0.4.eps'
